{
  "name": "@nanmo/vue-component-library",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/my-component-lib.umd.js",
  "module": "./dist/my-component-lib.es.js",
  "exports": {
    "import": "./dist/my-component-lib.es.js",
    "require": "./dist/my-component-lib.umd.js"
  },
  "types": "./dist/types/components/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --project ./tsconfig.lib.json",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.1",
    "@vue/test-utils": "^2.0.0-rc.18",
    "happy-dom": "^7.6.7",
    "lodash-es": "^4.17.21",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitepress": "1.0.0-alpha.28",
    "vitest": "^0.25.1",
    "vue-tsc": "^1.0.9"
  }
}
